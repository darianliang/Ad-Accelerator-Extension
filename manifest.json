{
  "manifest_version": 3,
  "name": "Ad Accelerator",
  "version": "1.0",
  "description": "Speeds through YouTube ads automatically so you can get back to watching faster.",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}